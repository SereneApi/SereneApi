<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>$id$</id>
    <version>$version$</version>
    <authors>$authors$</authors>
    <owners>$authors$</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>$licenseUrl$</licenseUrl>
    <projectUrl>$projectUrl$</projectUrl>
    <description>$description$</description>
    <tags>$tags$</tags>
    <repository type="$repositoryType$" url="$repositoryUrl$" />
    <dependencies>
      <group targetFramework=".NETStandard2.1">
        <dependency id="SereneApi" version="$version$" exclude="Build,Analyzers" />
        <dependency id="SereneApi.Extensions.DependencyInjection" version="$version$" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
</package>