    <None Remove="Configuration\Dictionary\**" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="DeltaWare.Dependencies" Version="5.0.0.7" />
    <PackageReference Include="DeltaWare.SDK.Core" Version="5.0.1.2" />
    <PackageReference Include="DeltaWare.SDK.Serialization.Types" Version="5.0.0.4" />
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="5.0.0" />
  </ItemGroup>

</Project>
